            "required": [
                "id",
                "nome",
                "descricao",
                "ativo",
                "colaboradores"
            ],
            "properties": {
                "id": {
                    "type": "string",
                    "default": "",
                    "title": "The id Schema",
                    "examples": [
                        "ZL.15046.ASD"
                    ]
                },
                "nome": {
                    "type": "string",
                    "default": "",
                    "title": "The nome Schema",
                    "examples": [
                        "ZL.15046 - Assistente a Docência"
                    ]
                },
                "descricao": {
                    "type": "string",
                    "default": "",
                    "title": "The descricao Schema",
                    "examples": [
                        "Assistente a Docência: ZL.15046 - Especialista em Tecnologias Educacionais e Educação a Distância"
                    ]
                },
                "ativo": {
                    "type": "boolean",
                    "default": false,
                    "title": "The ativo Schema",
                    "examples": [
                        true
                    ]
                },
                "colaboradores": {
                    "type": "array",
                    "default": [],
                    "title": "The colaboradores Schema",
                    "items": {
                        "type": "object",
                        "default": {},
                        "title": "A Schema",
                        "required": [
                            "username",
                            "nome_completo",
                            "email",
                            "vinculo_ativo"
                        ],
                        "properties": {
                            "username": {
                                "type": "string",
                                "default": "",
                                "title": "The username Schema",
                                "examples": [
                                    "chave"
                                ]
                            },
                            "nome_completo": {
                                "type": "string",
                                "default": "",
                                "title": "The nome_completo Schema",
                                "examples": [
                                    "nome da pessoa"
                                ]
                            },
                            "email": {
                                "type": "string",
                                "default": "",
                                "title": "The email Schema",
                                "examples": [
                                    "email@da.pessoa"
                                ]
                            },
                            "vinculo_ativo": {
                                "type": "boolean",
                                "default": false,
                                "title": "The vinculo_ativo Schema",
                                "examples": [
                                    true
                                ]
                            }
                        },
                        "examples": [
                            {
                                "username": "chave",
                                "nome_completo": "nome da pessoa",
                                "email": "email@da.pessoa",
                                "vinculo_ativo": true
                            }
                        ]
                    },
                    "examples": [
                        [
                            {
                                "username": "chave",
