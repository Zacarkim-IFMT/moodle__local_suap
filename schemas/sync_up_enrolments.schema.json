                "nome",
                "codigo",
                "descricao"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "default": 0,
                    "title": "The id Schema",
                    "examples": [
                        1
                    ]
                },
                "nome": {
                    "type": "string",
                    "default": "",
                    "title": "The nome Schema",
                    "examples": [
                        "Tecnologia em Redes de Computadores"
                    ]
                },
                "codigo": {
                    "type": "string",
                    "default": "",
                    "title": "The codigo Schema",
                    "examples": [
                        "00001"
                    ]
                },
                "descricao": {
                    "type": "string",
                    "default": "",
                    "title": "The descricao Schema",
                    "examples": [
                        "Tecnologia em Redes de Computadores - Nome Completo do Campus"
                    ]
                }
            },
            "examples": [
                {
                    "id": 1,
                    "nome": "Tecnologia em Redes de Computadores",
                    "codigo": "00001",
                    "descricao": "Tecnologia em Redes de Computadores - Nome Completo do Campus"
                }
            ]
        }
            "type": "array",
        "default": [],
